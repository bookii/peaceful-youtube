{
  "name": "Peaceful YouTube",
  "description": "Hide dislike button in YouTube",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/watch*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}